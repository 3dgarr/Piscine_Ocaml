module Set :
sig
  type 'a t

  val return : 'a -> 'a t
  val bind : 'a t -> ('a -> 'b t) -> 'b t

  val union : 'a t -> 'a t -> 'a t
  val inter : 'a t -> 'a t -> 'a t
  val diff : 'a t -> 'a t -> 'a t

  val filter : 'a t -> ('a -> bool) -> 'a t
  val foreach : 'a t -> ('a -> unit) -> unit
  val for_all : 'a t -> ('a -> bool) -> bool
  val exists : 'a t -> ('a -> bool) -> bool
end
